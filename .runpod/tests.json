{
  "tests": [
    {
      "name": "text_to_video_720p_test",
      "input": {
        "prompt": "A beautiful sunset over the ocean with waves gently crashing on the beach",
        "resolution_preset": "720p",
        "duration_seconds": 3.0,
        "fps": 24
      },
      "timeout": 300000
    },
    {
      "name": "image_to_video_480p_test",
      "input": {
        "prompt": "Camera slowly pans across the landscape",
        "image": "iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mNkYPhfDwAChwGA60e6kgAAAABJRU5ErkJggg==",
        "resolution_preset": "480p",
        "duration_seconds": 2.0,
        "fps": 16
      },
      "timeout": 300000
    },
    {
      "name": "vertical_video_test",
      "input": {
        "prompt": "Person walking down a busy street",
        "resolution_preset": "720p_vertical",
        "duration_seconds": 2.0,
        "fps": 20
      },
      "timeout": 300000
    },
    {
      "name": "square_video_test",
      "input": {
        "prompt": "Coffee cup with steam rising",
        "resolution_preset": "square",
        "duration_seconds": 2.0,
        "fps": 18
      },
      "timeout": 300000
    },
    {
      "name": "mock_generator_test",
      "input": {
        "prompt": "Test video generation with mock generator",
        "resolution_preset": "480p",
        "duration_seconds": 1.0,
        "fps": 12
      },
      "timeout": 60000
    },
    {
      "name": "custom_dimensions_test",
      "input": {
        "prompt": "Test with custom width and height",
        "width": 640,
        "height": 360,
        "duration_seconds": 1.5,
        "fps": 15
      },
      "timeout": 120000
    },
    {
      "name": "negative_prompt_test",
      "input": {
        "prompt": "A serene mountain landscape",
        "negative_prompt": "blurry, low quality, distorted",
        "resolution_preset": "480p",
        "duration_seconds": 2.0,
        "fps": 16,
        "guidance_scale": 7.0
      },
      "timeout": 180000
    },
    {
      "name": "high_fps_test",
      "input": {
        "prompt": "Fast-moving water cascading down rocks",
        "resolution_preset": "square_small",
        "duration_seconds": 1.0,
        "fps": 30
      },
      "timeout": 120000
    },
    {
      "name": "long_duration_test",
      "input": {
        "prompt": "Clouds slowly moving across the sky",
        "resolution_preset": "480p_vertical",
        "duration_seconds": 5.0,
        "fps": 12
      },
      "timeout": 600000
    },
    {
      "name": "seed_reproducibility_test",
      "input": {
        "prompt": "A rotating geometric pattern",
        "resolution_preset": "square",
        "duration_seconds": 1.5,
        "fps": 20,
        "seed": 42
      },
      "timeout": 180000
    },
    {
      "name": "error_handling_invalid_preset_test",
      "input": {
        "prompt": "This should fail",
        "resolution_preset": "invalid_preset",
        "duration_seconds": 1.0,
        "fps": 10
      },
      "timeout": 30000,
      "expect_error": true
    },
    {
      "name": "error_handling_empty_prompt_test",
      "input": {
        "prompt": "",
        "resolution_preset": "480p",
        "duration_seconds": 1.0,
        "fps": 10
      },
      "timeout": 30000,
      "expect_error": true
    },
    {
      "name": "error_handling_invalid_fps_test",
      "input": {
        "prompt": "Test invalid FPS",
        "resolution_preset": "480p", 
        "duration_seconds": 1.0,
        "fps": 70
      },
      "timeout": 30000,
      "expect_error": true
    }
