{
  "title": "WAN 2.2 Video (I2V & T2V) with LoRA",
  "description": "WAN 2.2 video generation with I2V & T2V support, LoRA integration, resolution presets (480p/720p/vertical/square), configurable duration & FPS, auto model downloads, 6 model variants (3.5GB-80GB VRAM).",
  "type": "serverless",
  "category": "video",
  "iconUrl": "https://raw.githubusercontent.com/nikserg/wan-video-runpod-serverless/refs/heads/main/logo.png",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 100,
    "presets": [
      {
        "name": "üöÄ Fast Testing (Mock Generator)",
        "defaults": {
          "WAN_MODEL_TYPE": "VACE-1.3B",
          "USE_MOCK_GENERATOR": "true"
        }
      },
      {
        "name": "‚ö° Ultra Fast I2V (3.5GB VRAM)",
        "defaults": {
          "WAN_MODEL_TYPE": "VACE-1.3B",
          "USE_MOCK_GENERATOR": "false"
        }
      },
      {
        "name": "üéØ Production RTX 4090 (24GB) - Recommended",
        "defaults": {
          "WAN_MODEL_TYPE": "I2V-14B-720P",
          "USE_MOCK_GENERATOR": "false"
        }
      },
      {
        "name": "üí® Speed Priority (17GB VRAM)",
        "defaults": {
          "WAN_MODEL_TYPE": "I2V-14B-480P",
          "USE_MOCK_GENERATOR": "false"
        }
      },
      {
        "name": "üîÑ Dual Mode T2V+I2V (24GB)",
        "defaults": {
          "WAN_MODEL_TYPE": "TI2V-5B",
          "USE_MOCK_GENERATOR": "false"
        }
      },
      {
        "name": "üèÜ Enterprise T2V Quality (80GB+)",
        "defaults": {
          "WAN_MODEL_TYPE": "T2V-A14B",
          "USE_MOCK_GENERATOR": "false"
        }
      },
      {
        "name": "üèÜ Enterprise I2V Quality (80GB+)",
        "defaults": {
          "WAN_MODEL_TYPE": "I2V-A14B",
          "USE_MOCK_GENERATOR": "false"
        }
      }
    ],
    "env": [
      {
        "key": "WAN_MODEL_TYPE",
        "input": {
          "name": "WAN Model Type",
          "type": "select",
          "description": "Choose WAN model variant based on your GPU VRAM",
          "options": ["VACE-1.3B", "I2V-14B-480P", "I2V-14B-720P", "TI2V-5B", "T2V-A14B", "I2V-A14B"],
          "default": "I2V-14B-720P"
        }
      },
      {
        "key": "LORA_URL_1",
        "input": {
          "name": "LoRA URL 1",
          "type": "string",
          "description": "First LoRA download URL (e.g., from CivitAI)",
          "default": ""
        }
      },
      {
        "key": "LORA_URL_2",
        "input": {
          "name": "LoRA URL 2",
          "type": "string",
          "description": "Second LoRA download URL (optional)",
          "default": ""
        }
      },
      {
        "key": "LORA_URL_3",
        "input": {
          "name": "LoRA URL 3",
          "type": "string",
          "description": "Third LoRA download URL (optional)",
          "default": ""
        }
      },
      {
        "key": "USE_MOCK_GENERATOR",
        "input": {
          "name": "Use Mock Generator",
          "type": "select",
          "description": "Use mock generator for testing (faster startup)",
          "options": ["false", "true"],
          "default": "false"
        }
      }
    ]
  }
}